<?xml version='1.0' encoding='utf-8'?>
<widget id="cn.inu1255.automan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>魂签</name>
    <description>
        自动签到.
    </description>
    <author email="929909260@qq.com" href="http://inu1255.cn">
        inu1255
    </author>
    <launch-url>http://</launch-url>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <chcp>
        <auto-download enabled="true" />
        <auto-install enabled="true" />
        <config-file url="http://automan.inu1255.cn/www/chcp.json" />
    </chcp>
    <allow-navigation href="http://*.inu1255.cn/*" />
    <allow-navigation href="http://192.168.1.119/*" />
    <allow-navigation href="http://192.168.0.102/*" />
    <plugin name="cn.inu1255.cordova.proxy" spec="../proxy" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.2" />
    <plugin name="cordova-plugin-local-notification" spec="^0.9.0-beta.2" />
    <plugin name="com.lampa.startapp" spec="https://github.com/lampaa/com.lampa.startapp.git" />
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
